.content {
  display: flex;
  flex-direction: row;
  font-family: "Microsoft YaHei"
}
text{
overflow: hidden;
white-space: nowrap;
text-overflow:ellipsis;
}
.left {
  width: 20%;
  font-size: 10px;
  height: 1500rpx;
  background-color: #F4F4F4;
}
.left view {
  text-align: center;
  height: 45px;
  line-height: 45px;
}
.select {
  background-color: #ffffff;
  border-left: 2px solid #36AE66;
  font-weight: bold;
  color: #36AE66;
}
.normal {
  background-color: #F4F4F4;
  border-bottom: 1px solid #f2f2f2;
}
.right {
  width: 70%;
  margin: 0px;
}
#tx{
   height: 115rpx;
  width: 160rpx;
}
.xq{
  height: 140rpx
}