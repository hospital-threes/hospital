<view class="main">
  <!--预约挂号 -->

  <view class="liebiao" wx:for="{{list1}}" wx:key="index" wx:for-item="item">
    <navigator url="" open-type="navigate">
      <view>
        <view class="yugh">
          <text>{{item.orderName}}</text>
        </view>
        <view class="ddh">
          <text>订单号</text>
        </view>
        <view class="ddhzssj">{{item.orderNum}}</view>
        <view class="yuan">￥{{item.payMoney}}</view>
        <view class="zfzt">待支付</view>
        <view class="date">{{item.appointmentTime}}</view>
        <view class="dates">{{item.timeframe}}</view>
        <view class="hospital">{{item.hospitalName}}</view>
        <view class="name">{{item.name}}</view>
        <view class="ll">
          <image src="/images/my_images/tip.png" class="user-avatars"></image>
        </view>
      </view>
    </navigator>
  </view>

